{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["esnext", "dom"],
    "types": ["cypress", "cypress-axe"],
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["../node_modules"]
}

